    "build": "npm run clean && npm run build:ts && npm run build:css && npm run cp:pack",
    "build:css": "node-sass src/style.scss build/style.css",
    "test": "jest --collectCoverage",
    "prepush": "npm run test",
    "clean": "rm -rf build && mkdir -p build",
    "cp:pack": "cp ./package.json ./build/package.json",
    "release:patch": "npm run build && npm version patch && npm run cp:pack && cd build && npm publish && git push --follow-tags",
    "release:minor": "npm run build && npm version minor && npm run cp:pack && cd build && npm publish && git push --follow-tags",
    "release:major": "npm run build && npm version major && npm run cp:pack && cd build && npm publish && git push --follow-tags"
  },
